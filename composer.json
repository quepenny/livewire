{
    "name": "quepenny/livewire",
    "description": "Livewire library for Quepenny projects",
    "type": "library",
    "require": {
        "php": "^8.3",
        "barryvdh/laravel-snappy": "^1.0",
        "ext-zip": "*",
        "ext-fileinfo": "*",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "laravel/framework": "^12.16.0",
        "laravel/jetstream": "^5.0",
        "laravel/socialite": "^5.6",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "laravel/scout": "^10.0",
        "wire-elements/modal": "^3.0.1",
    },
    "require-dev": {
        "laravel/pint": "^1.9",
        "phpunit/phpunit": "^11.3.1"
    },
    "autoload": {
        "psr-4": {
            "Quepenny\\Livewire\\": "src/",
            "Quepenny\\Livewire\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quepenny\\Livewire\\Providers\\AppServiceProvider",
                "Quepenny\\Livewire\\Providers\\EventServiceProvider",
                "Quepenny\\Livewire\\Providers\\MacroServiceProvider",
                "Quepenny\\Livewire\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Paul Ogbeiwi",
            "email": "developers@quepenny.com"
        }
    ]
}
